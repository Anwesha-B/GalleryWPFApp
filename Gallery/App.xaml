<Application x:Class="Gallery.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Gallery"
             xmlns:converter="clr-namespace:Gallery.Converter"
             StartupUri="View/GalleryOverview.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converter:ImageConverter x:Key="LocalImageConverter" />
            <BooleanToVisibilityConverter x:Key="BoolToVis" />
            <converter:DebugConverter x:Key="DebugConverter"/>
            <converter:PriceConverter x:Key="LocalPriceConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
